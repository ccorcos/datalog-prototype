{
	"name": "typescript-boilerplate",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"start": "run-p start-client start-server",
		"start-client": "webpack-dev-server --content-base build/client",
		"start-server": "nodemon --watch src/server --watch src/shared --exec 'node --inspect=5858 -r ts-node/register' ./src/server/main.ts",
		"build": "rm -rf dist && webpack -p",
		"postinstall": "patch-package",
		"build-patch": "patch-package slate"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@types/fs-extra": "^8.1.0",
		"@types/md5": "^2.2.0",
		"@types/morgan": "^1.9.0",
		"@types/uuid": "^7.0.3",
		"better-sqlite3": "^7.0.1",
		"diff-sequences": "^26.0.0",
		"fs-extra": "^9.0.0",
		"glamor": "^2.20.40",
		"is-hotkey": "^0.1.6",
		"lodash": "^4.17.15",
		"md5": "^2.2.1",
		"morgan": "^1.10.0",
		"patch-package": "^6.2.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"slate": "^0.58.1",
		"slate-history": "^0.58.1",
		"slate-react": "^0.58.1",
		"uuid": "^8.0.0",
		"ws": "^7.3.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^5.4.0",
		"@types/express": "^4.17.6",
		"@types/html-webpack-plugin": "^3.2.2",
		"@types/lodash": "^4.14.150",
		"@types/react": "^16.9.35",
		"@types/react-dom": "^16.9.8",
		"@types/webpack": "^4.41.12",
		"@types/ws": "^7.2.4",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.3.0",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"ts-loader": "^7.0.4",
		"ts-node": "^8.10.1",
		"typescript": "^3.9.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	}
}
